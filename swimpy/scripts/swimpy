#/usr/bin/env python

import swimpy.project
from modelmanager.settings import Function
from modelmanager.commandline import execute_from_commandline
from modelmanager.project import ProjectDoesNotExist

description = "SWIM python command line interface."

try:
    project = swimpy.project.Project()
    functions = {}
except ProjectDoesNotExist:
    functions = {'setup': Function(swimpy.project.setup)}
    project = None

print(execute_from_commandline(project, functions, description))
