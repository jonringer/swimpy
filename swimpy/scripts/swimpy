#!/usr/bin/env python
from modelmanager import project, settings, commandline
from swimpy.project import Project, setup
import swimpy

description = 'SWIMpy commandline interface\n\nDocumentation: %s' % swimpy.__docs__
try:
    commandline.CommandlineInterface(Project(), description=description).run()
except project.ProjectDoesNotExist:
    functions = {'setup': setup}
    commandline.CommandlineInterface(functions=functions, description=description).run()
